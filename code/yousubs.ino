  int imgX = 80;
  int imgY = 10;
  int imgSize = 88;

  int centerX = imgX + imgSize / 2;
  int centerY = imgY + imgSize / 2;
  TJpgDec.drawFsJpg(imgX, imgY, "/M81.jpg");

  for (int r = imgSize / 2; r < (sqrt(pow(imgSize, 2) + pow(imgSize, 2))) / 2; r++)
  {
    for (float angle = 0; angle < 360; angle += 0.5)
    {
      int x = centerX + r * cos(angle * PI / 180);
      int y = centerY + r * sin(angle * PI / 180);
      tft.drawPixel(x, y, TFT_BLACK);
    }
  }
}

StaticJsonDocument<1024> fetchYouTubeData(const char *endpoint, const String &queryParams, const char *apiKey)
{
  StaticJsonDocument<1024> jsonDoc;

  if (WiFi.status() == WL_CONNECTED)
  {
    HTTPClient http;
    String url = String(endpoint) + "?" + queryParams + "&key=" + String(apiKey);
    Serial.println("URL richiesta: " + url);

    if (WiFi.status() != WL_CONNECTED)
    {
      Serial.println("Connessione Wi-Fi persa!");
      return jsonDoc;
    }
    http.begin(url);
    int httpCode = http.GET();

    if (httpCode == HTTP_CODE_OK)
    {
      String payload = http.getString();
      Serial.println("JSON ricevuto:");
      Serial.println(payload);

      DeserializationError error = deserializeJson(jsonDoc, payload);
      if (error)
      {
        Serial.print("Errore di parsing JSON: ");
        Serial.println(error.c_str());
      }
    }
    else
    {
      Serial.print("Errore HTTP: ");
      Serial.println(httpCode);
    }

    http.end();
  }
  else
  {
    Serial.println("Wi-Fi non connesso!");
  }

  return jsonDoc;
}

void updateSubs()
{
  String queryParams = String("part=statistics&id=") + channelId;
  StaticJsonDocument<1024> jsonResponse = fetchYouTubeData("https://www.googleapis.com/youtube/v3/channels", queryParams, api_key);
  if (!jsonResponse.isNull())
  {
    int newSub = atoi(jsonResponse["items"][0]["statistics"]["subscriberCount"]);
    if (sub != newSub)
    {
      sub = newSub;
      tft.fillRect(0, 100, 240, 140, TFT_BLACK);
      tft.setFreeFont(FSS24);
      int nLen = tft.textWidth(String(sub));
      tft.drawString(String(sub), (tft.width() - nLen) / 2, tft.height() / 2);
      tft.drawString("Subscribers", (tft.width() - tft.textWidth("Subscribers", 4)) / 2, tft.height() - 30, 4);
    }
  }
  else
  {
    Serial.println("Errore nel JSON o valore assente!");
  }
}

void setup()
{
  Serial.begin(115200);
  tft.init();
  tft.setRotation(1);
  tft.fillScreen(TFT_BLACK);
  tft.setTextColor(TFT_WHITE, TFT_BLACK);
  tft.fillRoundRect(22, 96, 63, 43, 10, TFT_RED);
  tft.drawBitmap(0, 0, epd_bitmap_yousub, bitmapWidth, bitmapHeight, TFT_WHITE);
  delay(1000);

  EEPROM.begin(EEPROM_SIZE);
  readSettings();

  Serial.println("Connessione al Wi-Fi...");
  WiFi.begin(ssid, password);

  unsigned long startTime = millis();
  while (WiFi.status() != WL_CONNECTED && millis() - startTime < 10000)
  {
    Serial.print(".");
    delay(500);
  }

  if (WiFi.status() == WL_CONNECTED)
  {
    Serial.println("\nConnesso al Wi-Fi!");
    Serial.print("Indirizzo IP: ");
    Serial.println(WiFi.localIP());

    tft.fillScreen(TFT_BLACK);
    tft.setCursor(10, tft.height() / 2 - 10);
    tft.drawString("Indirizzo IP:", 10, 10, 4);
    tft.drawString(WiFi.localIP().toString(), 10, tft.height() / 2 - 20, 4);
    delay(5000);

    String queryParams = String("part=snippet&forHandle=") + nomeCanale;

    StaticJsonDocument<1024> jsonResponse = fetchYouTubeData("https://www.googleapis.com/youtube/v3/channels", queryParams, api_key);
    if (!jsonResponse.isNull())
    {
      if (jsonResponse.containsKey("items") && jsonResponse["items"][0].containsKey("snippet") && jsonResponse["items"][0]["snippet"].containsKey("thumbnails") && jsonResponse["items"][0]["snippet"]["thumbnails"].containsKey("default") && jsonResponse["items"][0]["snippet"]["thumbnails"]["default"].containsKey("url") && jsonResponse["items"][0].containsKey("id"))
      {

        const String imageUrl = jsonResponse["items"][0]["snippet"]["thumbnails"]["default"]["url"];
        channelId = String(jsonResponse["items"][0]["id"].as<const char *>());
        tft.fillScreen(TFT_BLACK);
        fetchUserImage(imageUrl);
        updateSubs();
      }
      else
      {
        tft.fillScreen(TFT_BLACK);
        tft.drawString("Missing keys...", 10, 10, 4);
      }
    }
    else
    {
      tft.fillScreen(TFT_BLACK);
      tft.drawString("Error...", 10, 10, 4);
    }
  }
  else
  {
    Serial.println("\nImpossibile connettersi al Wi-Fi. Attivazione modalità AP...");
    WiFi.softAP(ap_ssid, ap_password);
    Serial.print("Indirizzo IP AP: ");
    Serial.println(WiFi.softAPIP());

    tft.fillScreen(TFT_BLACK);
    tft.setCursor(10, tft.height() / 2 - 10);
    tft.drawString("Indirizzo IP AP:", 10, 10, 4);
    tft.drawString(WiFi.softAPIP().toString(), 10, tft.height() / 2 - 20, 4);
  }

  setupWebServer();
}

void loop()
{
  unsigned long currentMillis = millis();
  if (currentMillis - previousMillis >= interval)
  {
    previousMillis = currentMillis;
    updateSubs();
  }
  server.handleClient();
}
