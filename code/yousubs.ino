void saveToEEPROM(int addr, String value)
{
  for (int i = 0; i < value.length(); i++)
  {
    EEPROM.write(addr + i, value[i]);
  }
  EEPROM.write(addr + value.length(), '\0');
  EEPROM.commit();
}

void readFromEEPROM(int addr, char *buffer, int maxLen)
{
  for (int i = 0; i < maxLen; i++)
  {
    char c = EEPROM.read(addr + i);
    if (c == '\0')
    {
      buffer[i] = '\0';
      break;
    }
    buffer[i] = c;
  }
}

void readSettings()
{
  readFromEEPROM(WIFI_SSID_ADDR, ssid, MAX_LEN);
  readFromEEPROM(WIFI_PASS_ADDR, password, MAX_LEN);
  readFromEEPROM(NOME_CANALE_ADDR, nomeCanale, MAX_LEN);
  readFromEEPROM(API_KEY_ADDR, api_key, MAX_LEN);
}

bool tft_output(int16_t x, int16_t y, uint16_t w, uint16_t h, uint16_t *bitmap)
{
  if (y >= tft.height())
    return 0;
  tft.pushImage(x, y, w, h, bitmap);
  return 1;
}

